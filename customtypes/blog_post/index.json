{
  "format": "custom",
  "id": "blog_post",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "category": {
        "type": "Text",
        "config": {
          "label": "Category",
          "placeholder": ""
        }
      },
      "excerpt_meta_description": {
        "type": "StructuredText",
        "config": {
          "label": "Excerpt / Meta Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "publication_date": {
        "type": "Date",
        "config": {
          "label": "Publication Date",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "rich_text": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            },
            "embed": {
              "type": "SharedSlice"
            },
            "code_block": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
